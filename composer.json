{
    "name": "netsper/laravel-notify-track",
    "description": "NotifyTrack is a versatile Laravel package for tracking notifications (email, SMS, etc.) sent to various models. It logs and retrieves detailed information, providing insights into the type, date, and frequency of notifications sent.",
    "keywords": [
        "Netsper",
        "laravel-notify-track"
    ],
    "homepage": "https://github.com/netsper/laravel-notify-track",
    "license": "MIT",
    "authors": [
        {
            "name": "Barış",
            "email": "baris@barisaksu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Netsper\\LaravelNotifyTrack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netsper\\LaravelNotifyTrack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
